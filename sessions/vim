let SessionLoad = 1
if &cp | set nocp | endif
let s:cpo_save=&cpo
set cpo&vim
nmap gx <Plug>NetrwBrowseX
nnoremap <silent> <Plug>NetrwBrowseX :call netrw#NetrwBrowseX(expand("<cWORD>"),0)
map <silent> <F3> :if &guioptions =~# 'T' |set guioptions-=T |set guioptions-=m |else |set guioptions+=T |set guioptions+=m |endif
let &cpo=s:cpo_save
unlet s:cpo_save
set autoindent
set background=dark
set backspace=indent,eol,start
set complete=.,w,b,k,t,i
set completeopt=longest,menu
set expandtab
set fileencodings=utf-8,gbk,chinese,latin1
set formatoptions=tcqmM
set guioptions=aegirLtTm
set helplang=en
set hidden
set history=500
set hlsearch
set ignorecase
set iminsert=0
set imsearch=0
set iskeyword=@,48-57,_,192-255,_,$,@,%,#,-
set laststatus=2
set listchars=tab:|\ ,nbsp:%,trail:-
set matchpairs=(:),{:},[:],<:>
set mouse=a
set ruler
set rulerformat=%15(%c%V\ %p%%%)
set shiftwidth=4
set showcmd
set smartindent
set smarttab
set softtabstop=4
set statusline=\ [File]\ %F%m%r%h%y[%{&fileformat},%{&fileencoding}]\ %w\ \ [PWD]\ %r%{GetPWD()}%h\ %=\ [Line]%l/%L\ %=[%P]
set tabpagemax=15
set tabstop=4
set termencoding=utf-8
set undodir=~/tmp/vim/undofile/
set undofile
set whichwrap=b,s,<,>,[,]
set wildmenu
set window=37
set nowritebackup
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd ~/
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +1 .vimrc
badd +1 repository/Vim/vimrc
silent! argdel *
edit .vimrc
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
set nosplitbelow
set nosplitright
wincmd t
set winheight=1 winwidth=1
exe 'vert 1resize ' . ((&columns * 76 + 77) / 155)
exe 'vert 2resize ' . ((&columns * 78 + 77) / 155)
argglobal
